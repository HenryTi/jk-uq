OPEN TUID Chemical (
    id,
    no bigint not null,
    CAS char(20),
    description char(1000),
    descriptoinCN char(1000),
    molecularFomula char(200),
    molecularWeight dec(12, 4),
    mdlNumber char(12),
    unique (no),
    index cas(cas) unique,
);

