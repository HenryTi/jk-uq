{
    "couponOrCreditsCreated": [
        "couponOrCredits: id",
        "types: string",
        "creator: id",
        "createTime: string"
    ],
    "creditsUsedByWebUser": [
        "orderId: string",
        "webUser: id",
        "amount: number",
        "currency: id",
        "point: number",
        "coupon: id",
        { "name": "orderItems", "type": "array", "fields": "orderItem" }
    ],
    "creditsUsedByCustomer": [
        "orderId: string",
        "customer: id",
        "amount: number",
        "currency: id",
        "point: number",
        "coupon: id",
        { "name": "orderItems", "type": "array", "fields": "orderItem" }
    ],
   "orderItem": [
        { "name": "row", "type": "number" },
        { "name": "orderItemId", "type": "string" },
        { "name": "point", "type": "number" }
    ],
   "creditsDrawedByWebUser": [
        "webUser: id",
        "coupon: id",
        "createDate: string",
        "expiredDate: string"
    ],
    "creditsDrawedByCustomer": [
        { "name": "customer", "type": "id" },
        { "name": "webUser", "type": "id" },
        { "name": "coupons", "type": "array", "fields": "couponDrawed" }
    ],
    "couponDrawed": [
        "coupon: id",
        "couponType: id",
        "couponCode: number",
        "createDate: string",
        "expiredDate: string"
    ], 
    "couponUsed": [
        "coupon: id",
        "saleOrder: string",
        "usedDate: string"
    ],
    "getWebUserAllCredits": {
         "param": [ {"name": "webUser", "type": "id"} ],
         "returns": [
              "webUser:id",
              "creditsDrawed: couponDrawed", "creditsUsed: couponUsed",
              "couponDrawed: couponDrawed", "couponUsed: couponUsed"
         ]
    },
    "getMatchedCredits": {
        "param": [
            {"name": "customer", "type": "id"}, 
            {"name": "orderItemId", "type": "string"}, 
            {"name": "createDate", "type": "string"}
        ],
        "returns": ["credits:id"]
    },
    "order-coupon": [
        "id: id",
        "couponUser: id",
        "coupon: id",
        "detail: order-coupon-detail"
    ],
    "order-coupon-detail": [
        "id: id"
    ],
    "customer-bounded": [
        "customer: id",
        "salesman: id"
    ]
}