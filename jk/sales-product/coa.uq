--lot批次信息,COA信息
TUID Lot (
    id,
    product ID productx not null,                -- 产品编号
	lotnumber char(50),                         -- lot号
    index idx_origin_lotnr(product,lotnumber),
);

--COA信息
Map COA (
    key lot id Lot not null,
    content text,                               -- COA信息
    version char(50),                           -- COA版本
    issuer char(50),                            -- COA签发人
    issueDate date not null,                    -- COA签发日期
);

