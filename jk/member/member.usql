OPEN TUID Member(
    id,
    userId bigint not null,
    recommendationCode char(10),
    unique(recommendationCode),
);

OPEN MAP MemberRecommender(
    key member ID Member,
    key referrer ID Member,
);

BOOK Point(
    key member ID Member,
    point dec(10, 0),
);

