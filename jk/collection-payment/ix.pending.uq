
IDX DxOrderDetail (
	id,									-- order detail
	receive DEC(12,2) DEFAULT 0,		-- 应收
	receiveDone DEC(12,2) SUM,	-- 已收
	receiveReturn DEC(12,2) SUM,		-- 应退
	receiveReturnDone DEC(12,2) SUM,	-- 已退
	invoice DEC(12,2) DEFAULT 0,		-- 应开票
	invoiceDone DEC(12,2) DEFAULT 0,	-- 已开票
	invoiceReturn DEC(12,2) SUM,		-- 应退票
	invoiceReturnDone DEC(12,2) SUM,	-- 已退票
);